/* pages/ordinary/managerSuccess/managerSuccess.wxss */
page{
  background: #f7f7f7;
}
.searchInputViewAll{
  width: 750rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  padding-bottom: 12rpx;
}
.searchInputView{
  display: flex;
  align-items: center;
  background-color: rgba(247, 247, 247, 1);
  margin-left: 15rpx;
  padding-left: 15rpx;
  width: 705rpx;
}
.searchInputView .searchInput{
  width: 665rpx;
  height: 76rpx;
  background-color: rgba(247, 247, 247, 1);
  font-family: PingFangSC-regular;
  font-size: 28rpx;
}
.managerSuccessList{
  display: flex;
  margin-top: 16rpx;
  padding:20rpx;
  width: 710rpx;
  height: 160rpx;
  background: #FFFFFF;
}
.managerSuccessListImg{
  width: 160rpx;
  height: 160rpx;
  margin-right: 14rpx;
}
.managerSuccessListImg image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
  background: #DDDDDD;
}
.managerSuccessListText{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.managerSuccessListName{
  color: rgba(48, 49, 51, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  margin-bottom: 10rpx;
}
.managerSuccessListNum{
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}