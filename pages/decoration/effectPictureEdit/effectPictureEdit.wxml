  <view class="effectPictureEditInput">
    <input placeholder="风格名称（可选/可手输）"></input>
    <picker bindchange="bindPickerChange" value="{{index}}" range="{{decorationArray.textList}}">
      <view><image src="../../../image/icon/more.svg"></image></view>
    </picker>
  </view>
  <view class="effectPictureEditInput"><input placeholder="输入复制的VR网址"></input></view>
  <view class="effectPictureEditInput">
    <input placeholder="装修清单" disabled="true"></input>
    <view  bindtap="goUrl"  data-url='/pages/decoration/effectPictureChecklist/effectPictureChecklist'><image src="../../../image/icon/more.svg"></image></view>
  </view>
  <view class="effectPictureEditInput"><input placeholder="请填写空间名称"></input></view>
</view>
