/* pages/decoration/orderListDetails/orderListDetails.wxss */
page{
  background: #f7f7f7;
}
.manageList{
  width: 100%;
  height: 200rpx;
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
  border-radius: 8rpx;
  padding: 0 20rpx;
  margin-bottom: 16rpx;
}
.manageList .listInfo{
  width: 100%;
  height: 160rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
  top: 18rpx;
}
.manageList .listInfo>image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.manageList .listInfo>view{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.manageList .listInfo>view>view{
  display: inline-flex;
  justify-content: space-between;
  align-items: center;
  width: 540rpx;
}
.manageList .listInfo>view .name{
  height: 36rpx;
  font-size: 28rpx;
  color: #303133;
  line-height: 28rpx;
}
.manageList .listInfo>view .name image{
  width: 36rpx;
  height: 36rpx; 
}
.manageList .listInfo>view .tags{
  height: 40rpx;
  line-height: 40rpx;
}
.manageList .listInfo>view .tags>view:nth-child(1){
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.manageList .listInfo>view .tags>view:nth-child(2){
  font-size: 28rpx;
  color: #0081FF;
}
.manageList .listInfo>view .phone{
  font-size: 24rpx;
  color: #909399;
  line-height: 24rpx;
}
.manageList .listInfo>view .develop{
  font-size: 24rpx;
  line-height: 24rpx;
  height: 32rpx;
  color: #909399;
}
.manageList .listInfo>view .develop text{
  color: #0081FF;
}
.manageList .listInfo>view .develop>view:nth-child(2){
  height: 100%;
  display: inline-flex;
  align-items: center;
}
.manageList .listInfo>view .develop image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
  margin-top: 4rpx;
}
.constructionDetailsPickerAll{
  margin-top: 15rpx;
}
.constructionDetailsPicker{
  width: 720rpx;
  padding: 0 15rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  align-items: center;
  border-bottom:1rpx solid #f7f7f7;
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  font-weight: 550;
}
.constructionDetailsPickerData{
  display: flex;
  height: 80rpx;
  align-items: center;
}
.constructionDetailsPickerData image{
  width: 35rpx;
  height: 35rpx;
  margin-left: 10rpx;
}
.orderListSchedule{
  margin-top: 15rpx;
  margin-bottom: 120rpx;
  background: #FFFFFF;
  padding: 0 20rpx 0 35rpx;
  width: 695rpx;
}
.orderListScheduleTitle{
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
  align-items: center;
}
.orderListScheduleTitle>view:nth-child(1){
  color: rgba(0, 129, 255, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular
}
.orderListScheduleTitle>view:nth-child(2){
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 8rpx;
  background-color: rgba(79, 163, 251, 1);
  color: rgba(255, 255, 255, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
}
.orderListScheduleListTitle{
  display: flex;
  align-items: center;
  height: 60rpx;
  justify-content: space-between;
  padding-left: 70rpx;
  position: relative;
}
.orderListScheduleListTitle::before{
  display: inline-block;
  content: '';
  width: 35rpx;
  height: 35rpx;
  border-radius: 50%;
  background: #4FA3FB;
  position: absolute;
  left: 0;
  top: 15rpx;
}
.orderListScheduleListTitle>view:nth-child(1){
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular
}
.orderListScheduleListTitle>view:nth-child(2){
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 8rpx;
  background-color: rgba(255, 255, 255, 1);
  color: rgba(144, 147, 153, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
  border: 1rpx solid rgba(178, 178, 178, 1);
}
.orderListScheduleListBody{
  height: 80rpx;
  padding-left: 53rpx;
  line-height: 80rpx;
  margin-left: 17rpx;
  position: relative;
}
.orderListScheduleListBody view{
  height: 106rpx;
  width: 4rpx;
  background:#4FA3FB;
  position: absolute;
  top: -12rpx;
  left: 0;
}
.orderListScheduleList:last-child .orderListScheduleListBody view{
  height: 0;
}
.orderListScheduleListBody text{
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.orderListScheduleListBody text:nth-child(3){
  color: rgba(0, 129, 255, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  margin-left: 20rpx;
}
.orderListScheduleListTwo .orderListScheduleListBody view{
  background:#FF7200;
}
.orderListScheduleListTwo .orderListScheduleListTitle::before{
  background: transparent;
  border: 4rpx solid #FF7200;
  width: 30rpx;
  height: 30rpx;
}
.orderListScheduleListTwo .orderListScheduleListTitle>view:nth-child(2){
  border: 1rpx solid #FF7200;
  color: #FF7200;
}