/* pages/decoration/commodityList/commodityList.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #F7F7F7;
}
.scroll{
  width: 100%;
  height: calc(100% - 100rpx);
  box-sizing: border-box;
}
.commissionType{
  height: 100rpx;
  display: flex;
  background: #FFFFFF;
  align-items: center;
  width: 700rpx;
  padding:0 25rpx;
}
.commissionType view{
  color: rgba(144, 147, 153, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.commissionType view image{
  width: 30rpx;
  height: 30rpx;
  transform: rotate(90deg);
  vertical-align:middle;
  margin-left: 10rpx;
  margin-right: 40rpx;
  transition: .3s all linear;
}
.commissionType .nowCommissionType{
  color: #0081FF;
  font-weight: 550;
}