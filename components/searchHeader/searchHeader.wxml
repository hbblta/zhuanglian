<!--pages/ordinary/components/searchHeader/searchHeader.wxml-->
<view class="indexHeader">
    <view class="searchInputView">
      <icon class="icon-small" type="search" size="20"></icon>
      <input class="searchInput" type='text' confirm-type='search' placeholder='{{title}}' />
    </view>
    <image bindtap="goUrl"  data-url='/pages/decoration/editShop/editShop'  src="/image/icon/edit.png"></image>
    <button  open-type='share'><image  src="/image/icon/share.png"></image></button>
    <image  src="/image/icon/QRCode.png"></image>
</view>
