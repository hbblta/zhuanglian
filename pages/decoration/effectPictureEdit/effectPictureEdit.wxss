/* pages/decoration/effectPictureEdit/effectPictureEdit.wxss */
page{
  background: #f7f7f7;
}
.effectPictureEditInput{
  margin-top: 15rpx;
  width: 700rpx;
  padding-left: 25rpx;
  padding-right: 25rpx;
  height: 110rpx;
  background: #FFFFFF;
  display: flex;
  align-items: center;
}
.effectPictureEditInput input{
  width: 670rpx;
  height: 110rpx;
  font-size: 28rpx;
  font-family: Arial;
}
.effectPictureEditInput view{
  height: 110rpx;
  display: flex;
  align-items: center;
}
.effectPictureEditInput image{
  width: 30rpx;
  height: 30rpx;
}
.effectPictureEditMain{
  background: #FFFFFF;
  padding-left: 25rpx;
  padding-right: 25rpx;
  border-bottom: 1rpx solid #f7f7f7;
  width: 700rpx;
}
.effectPictureEditMainTitle{
  color: rgba(59, 59, 59, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  background: #FFFFFF;
}
.effectPictureEditMainList{
  display: flex;
  flex-wrap: wrap;
}
.effectPictureEditMainListData{
  width: 200rpx;
  margin-top: 20rpx;
  margin-right: 30rpx;
}
.effectPictureEditMainListData image{
  height: 200rpx;
  width: 200rpx;
}
.effectPictureEditMainListData input{
  color: rgba(144, 147, 153, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.effectPictureEditMainOne{
  padding-bottom: 45rpx;
}
.effectPictureEdidMore{
  height: 117rpx;
  width: 720rpx;
  margin-left: 30rpx;
  padding-top: 15rpx;
}
.effectPictureEdidMore image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
  vertical-align:middle;
}
.effectPictureEdidMore{
  color: rgba(0, 129, 255, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.effectPictureEditButtons{
  height: 100rpx;
  width: 735rpx;
  padding-right: 15rpx;
  display: flex;
  align-items: center;
  flex-direction: row-reverse ;
  background: #FFFFFF;
}
.effectPictureEditButtons view{
  color: #909399 ;
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
  height: 60rpx;
  width: 156rpx;
  background: #FFFFFF;
  border: 1rpx solid rgba(0, 129, 255, 1);
  border-radius: 40rpx;
  line-height: 60rpx;
  position: relative;
}
.effectPictureEditButtons view:nth-child(2){
  position: relative;
  left: 40rpx;
}
.effectPictureEditButtons .nowButtons{
  color: rgba(255, 255, 255, 1)!important;
  background: #0081FF!important;
  z-index: 1!important;
}
.effectPictureEditSet{
  width: 750rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: rgba(0, 129, 255, 1);
  color: rgba(255, 255, 255, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
}