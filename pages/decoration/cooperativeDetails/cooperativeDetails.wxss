/* pages/decoration/cooperativeDetails/cooperativeDetails.wxss */
page{
  background: #f7f7f7;
  height: 100%;
}
.materialOrderList{
  display: flex;
  width: 710rpx;
  height: 160rpx;
  padding: 20rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}
.materialOrderListImg{
  width: 160rpx;
  height: 160rpx;
}
.materialOrderListImg image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
  background: #DDDDDD;
}
.materialOrderListText{
  height: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 95%;
  margin-left: 5%;
}
.materialOrderListName{
  display: flex;
  height: 120rpx;
  justify-content: space-between;
  flex-direction: column;
}
.materialOrderListName view{
  color: #A7A9AE;
  font-size: 24rpx;
  font-family: PingFangSC-regular
}
.materialOrderListName>view:nth-child(1){
  color: rgba(48, 49, 51, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  font-weight: 550;
}
.materialOrderListName view:nth-child(3){
  display: flex;
  justify-content: space-between;
}
.materialOrderListName view:nth-child(3) image{
  width: 35rpx;
  height: 35rpx;
}
.generalSelectionBody{
  display: flex;
  height: calc(100% - 200rpx);
  overflow: hidden;
}
.generalSelectionlist{
  width: 200rpx;
  margin-top: 15rpx;
  background: #FFFFFF;
  margin-right: 18rpx;
}
.generalSelectionlist>view{
  width: 200rpx;
  height: 120rpx;
  text-align: center;
  font-family: Arial;
  font-size: 28rpx;
  color: #909399;
}
.generalSelectionlist view view{
  height: 60rpx;
  line-height: 60rpx;
}
.generalSelectionlist .generalSelectionCurrent{
  background: #0081FF;
  color: #FFFFFF;
  font-weight: 550;
}
.effectPictureChecklistData{
  overflow: auto;
}
.constructionSiteDetaViewOne{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 480rpx;
  height: 40rpx;
  padding: 15rpx;
  margin-top: 15rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}
.constructionSiteDeta{
  display: flex;
  align-items: center;
  height: 60rpx;
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.constructionSiteDetaViewOne image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
  transform: rotate(90deg);
}
.constructionSiteDetaNum{
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.distributionDetailsList{
  height: 160rpx;
  padding: 20rpx;
  border-radius: 8rpx;
  background: #FFFFFF;
  display: flex;
  margin-top: 15rpx;
}
.distributionDetailsListHead{
  margin-right: 20rpx;
  height: 160rpx;
  width: 160rpx;
  background: #DDDDDD;
}
.distributionDetailsListHead image{
  height: 160rpx;
  width: 160rpx;
}
.distributionDetailsListText{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 160rpx;
}
.distributionDetailsListText view{
  color: rgba(144, 147, 153, 1);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.distributionDetailsListText view:nth-child(1){
  color: rgba(48, 49, 51, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.distributionDetailsListText view:nth-child(2) image{
  width: 30rpx;
  height: 30rpx;
}
.searchInputViewBody{
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  background: #FFFFFF;
  padding: 12rpx;
  width: 486rpx;
}
.searchInputView{
  display: flex;
  align-items: center;
  background-color: rgba(247, 247, 247, 1);
  padding-left: 20rpx;
  width: 466rpx;
}
.searchInputView .searchInput{
  width: 430rpx;
  height: 76rpx;
  border-radius: 12rpx;
  background-color: rgba(247, 247, 247, 1);
  font-family: PingFangSC-regular;
  font-size: 28rpx;
}
.constructionSiteDetaViewTwo{
  justify-content: space-around;
}
.constructionSiteDetaViewTwo .constructionSiteDetaNumTwo{
  color: #0081FF;
}