/* pages/decoration/commodityDetalis/commodityDetalis.wxss */
page{
  background: #f7f7ff;
}
.swiperBox{
  height: 560rpx;
}
.swiperBox swiper{
  height: 100%;
  width: 100%;
}
.swiperBox swiper image{
  display: block;
  width: 100%;
  height: 100%;
}
.shopMapPhone{
  background: #FFFFFF;
  margin-top: 15rpx;
}
.shopMapPhoneView{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 15rpx;
  width: 720rpx;
  border-bottom: 1rpx solid #f7f7ff;
  color: rgba(85, 85, 85, 100);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.shopMapPhoneView image{
  width: 80rpx;
  height: 80rpx;
}
.brandTitle,.brandExplain{
  background: #ffffff;
  padding: 15rpx;
  width: 720rpx;
}
.brandTitleText,.brandExplain view:nth-child(1){
  color: rgba(48, 49, 51, 100);
  font-size: 32rpx;
  font-family: PingFangSC-bold;
  padding: 15rpx 0;
}
.brandPrice text:nth-child(1){
  color: rgba(255, 114, 0, 100);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
  margin-right: 30rpx;
}
.brandPrice text:nth-child(2){
  color: rgba(133, 133, 133, 100);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.brandTitleModel{
  color: rgba(144, 147, 153, 100);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.brandTitleModelNum{
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  color: rgba(144, 147, 153, 100);
}
.brandExplain{
  margin-top: 20rpx;
}
.brandExplain view:nth-child(2){
  color: rgba(144, 147, 153, 100);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
}
.brandExplain view:nth-child(3){
  color: rgba(144, 147, 153, 100);
  font-size: 24rpx;
  font-family: PingFangSC-regular;
  margin-top: 15rpx;
}