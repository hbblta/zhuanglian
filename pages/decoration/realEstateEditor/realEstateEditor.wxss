/* pages/decoration/realEstateEditor/realEstateEditor.wxss */
page{
  background: #f7f7f7;
  height: 100%;
}
.generalSelectionBody{
  display: flex;
  height: 76%;
  overflow: hidden;
}
.generalSelectionlist{
  width: 200rpx;
  margin-top: 15rpx;
  background: #FFFFFF;
  margin-right: 18rpx;
}
.generalSelectionlist view{
  width: 200rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-family: Arial;
  font-size: 28rpx;
  color: #909399;
  position: relative;
}
.generalSelectionlist .generalSelectionCurrent{
  background: #0081FF;
  color: #FFFFFF;
  font-weight: 550;
}
.realEstateEditorMoer{
  color: rgba(0, 129, 255, 1)!important;
  font-size: 24rpx;
}
.generalSelectionlistText{
  width: 750rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: rgba(0, 129, 255, 1);
  color: rgba(255, 255, 255, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
}
.realEstateEditorAllName{
  width: 470rpx;
  padding: 20rpx;
  height: 70rpx;
  font-size: 28rpx;
  font-family: Arial;
  margin-top: 15rpx;
  border-radius: 8rpx;
  background: #FFFFFF;
}
.realEstateEditorAllName input{
  height: 70rpx;
}
.realEstateEditorAllImg{
  background: #FFFFFF;
  padding: 0 20rpx 10rpx;
  width: 470rpx;
  margin-top: 15rpx;
}
.realEstateEditorAllImgTitle{
  display: flex;
  justify-content: space-between;
  height: 110rpx;
  align-items: center;
}
.realEstateEditorAllImgTitle text:nth-child(1){
  color: rgba(48, 49, 51, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.realEstateEditorAllImgTitle text:nth-child(2){
  color: rgba(191, 194, 204, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.realEstateEditorAllImgList{
  display: flex;
  flex-wrap: wrap;
}
.effectPictureEditButtons{
  height: 100rpx;
  width: 510rpx;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  flex-direction: row-reverse ;
}
.effectPictureEditButtons view{
  color: #909399 ;
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
  height: 60rpx;
  width: 156rpx;
  background: #FFFFFF;
  border: 1rpx solid rgba(0, 129, 255, 1);
  border-radius: 40rpx;
  line-height: 60rpx;
  position: relative;
}
.effectPictureEditButtons view:nth-child(2){
  position: relative;
  left: 40rpx;
}
.effectPictureEditButtons .nowButtons{
  color: rgba(255, 255, 255, 1)!important;
  background: #0081FF!important;
  z-index: 1!important;
}
.realEstateEditorMore{
  width: 470rpx;
  padding: 20rpx;
  height: 70rpx;
  font-size: 28rpx;
  font-family: Arial;
  margin-top: 15rpx;
  border-radius: 8rpx;
  background: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.realEstateEditorMore view{
  font-size: 28rpx;
  font-family: Arial;
}
.realEstateEditorMoreImg{
  width: 30rpx;
  height: 30rpx;
}
.realEstateEditorMoreDelete{
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  left: 5rpx;
  top: 5rpx;
}
.generalSelectionlistDelete{
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  right: 5rpx;
  top: 5rpx;
}
.effectPictureReleaseAdd{
  height: 100rpx;
  display: flex;
  align-items: center;
  color: rgba(0, 129, 255, 1);
  font-size: 28rpx;
  width: 470rpx;
  font-family: PingFangSC-regular;
}
.effectPictureReleaseAdd image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 12rpx;
}