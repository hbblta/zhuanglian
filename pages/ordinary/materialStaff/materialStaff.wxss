/* pages/ordinary/materialStaff/materialStaff.wxss */
.applicationBk{
  width: 100%;
  height: 240rpx;
  position: absolute;
  z-index: -1;
  bottom: 0;
}
.decorationStaffInput input{
  margin-top: 50rpx;
  width: 640rpx;
  margin-left: 25rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 80rpx;
  border-radius: 8rpx;
  background-color: rgba(247, 247, 247, 1);
  font-size: 28rpx;
  font-family: Arial;
}
.decorationStaffPact{
  margin-left: 25rpx;
  display: flex;
  height: 50rpx;
  margin-top: 50rpx;
  align-items: center;
}
.decorationStaffPact image{
  height: 50rpx;
  width: 50rpx;
  margin-right: 20rpx;
}
.decorationStaffPact text:nth-child(2){
  color: rgba(146, 146, 146, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.decorationStaffPact text:nth-child(3){
  color: #FF7C13;
  font-size: 28rpx;
  font-family: PingFangSC-regular;
}
.decorationStaffButton{
  margin-left: 25rpx;
  margin-top: 50rpx;
  width: 700rpx;
  height: 90rpx;
  line-height:90rpx;
  border-radius: 8rpx;
  background-color: rgba(255, 255, 255, 0);
  color: rgba(0, 129, 255, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
  border: 1px solid rgba(0, 129, 255, 1);
}