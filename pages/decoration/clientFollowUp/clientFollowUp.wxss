page{
  height: 100%;
  background: #f7f7f7;
  border-top: 1px solid transparent;
  box-sizing: border-box;
}
.user{
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  margin-top: 14rpx;
  margin-bottom: 18rpx;
}
.generalSelectionBody{
  display: flex;
  height: 100%;
  overflow: hidden;
}
.generalSelectionlist{
  width: 200rpx;
  background: #FFFFFF;
  margin-right: 18rpx;
}
.generalSelectionlist .generalSelectionlistView{
  width: 200rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-family: Arial;
  font-size: 28rpx;
  color: #909399;
}
.generalSelectionlist .generalSelectionCurrent{
  background: #0081FF;
  color: #FFFFFF;
  font-weight: 550;
}
.searchInputViewBody{
  background: #FFFFFF;
  width: 510rpx;
  border-radius: 8rpx;
}
.searchInputView{
  display: flex;
  align-items: center;
  background-color: rgba(247, 247, 247, 1);
  padding-left: 20rpx;
  width: 466rpx;
}
.searchInputView .searchInput{
  width: 430rpx;
  height: 76rpx;
  border-radius: 12rpx;
  background-color: rgba(247, 247, 247, 1);
  font-family: PingFangSC-regular;
  font-size: 28rpx;
}
.scroll{
  width: 100%;
  box-sizing: border-box;
  height: calc(100% - 350rpx);
}
.bottom-btn{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  margin-top: 16rpx;
}